@page "/search"

<h3>SearchPage</h3>
@foreach(int i in sortedNumbers)
{
    <p>@i</p>
}




@code {

    List<int> sortedNumbers { get; set; }

    protected override Task OnInitializedAsync()
    {

        var randomNumbers = new List<int> { 5, 4, 5, 7, 6, 9, 4, 1, 1, 3, 4, 50, 56, 41 };

        sortedNumbers = MergeSort.GenericMergeSort<int>(randomNumbers);
        return base.OnInitializedAsync();
    }

}

